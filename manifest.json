{
    "manifest_version":2,
    "name":"Page Font Style Extension",
    "version":"1.0",
    "description":"Changes Page style on a specific page",
    "icons":{
        "128":"assests/icon128.png",
        "48":"assests/icon48.png",
        "16":"assests/icon16.png"
    },
    "page_action":{
        "default_icon":"assests/icon16.png",
        "default_popup":"popup.html",
        "default_title":"PageFontStyle"
    },
    "background":{
        "scripts":["eventPage.js"],
        "persistent":false
    },
    "permissions":[
        "tabs",
        "http://developer.chrome.com/*"
    ],
    "content_scripts":[{
        "matches":["https://developer.chrome.com/*"],
        "js":["content.js","jquery-3.1.0.min.js"],
        "css":["content.css"]
    }]
}